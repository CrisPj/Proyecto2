<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Proyecto2Bundle\Entity\Ciudad" table="ciudad">
    <id name="idCiudad" type="integer" column="id_ciudad">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ciudad_id_ciudad_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="ciudad" type="string" column="ciudad" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
