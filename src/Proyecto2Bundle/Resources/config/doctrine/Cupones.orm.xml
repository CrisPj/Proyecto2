<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Proyecto2Bundle\Entity\Cupones" table="cupones">
    <id name="idCupon" type="integer" column="id_cupon">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="cupones_id_cupon_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="cupon" type="string" column="cupon" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="boolean" column="estado" nullable="true"/>
  </entity>
</doctrine-mapping>
